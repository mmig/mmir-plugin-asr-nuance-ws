{
  "name": "mmir-plugin-asr-nuance-ws",
  "version": "1.0.0",
  "description": "Cordova plugin for the MMIR framework that allows Automatic Speech Recognition (ASR) via Nuance WebSocket services (MIX)",
  "main": "./www/asrNuanceWs.js",
  "directories": {
    "lib": "./www"
  },
  "types": "index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "createplugincompat ./ && pluginexport ./"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab-cos.b.dfki.de/mmir/mmir-plugin-asr-nuance-ws.git"
  },
  "keywords": [
    "asr",
    "speech",
    "recognition",
    "nuance",
    "ws",
    "websockets"
  ],
  "author": "russa",
  "license": "MIT",
  "dependencies": {
    "mmir-plugin-encoder-speex": "git+https://gitlab-cos.b.dfki.de/mmir/mmir-plugin-encoder-speex.git"
  },
  "peerDependencies": {
    "mmir-lib": "git+https://gitlab-cos.b.dfki.de/mmir/dev_mmir-lib.git#develop/webpack"
  },
  "cordova": {
    "id": "mmir-plugin-asr-nuance-ws",
    "platforms": []
  },
  "engines": {
    "cordovaDependencies": {
      "1.0.0": {
        "git+https://gitlab-cos.b.dfki.de/mmir/mmir-plugin-encoder-speex.git": ">=1.0.0"
      }
    }
  },
  "mmir": {
    "compat": {
      "./www/asrNuanceWs.js": {
        "file": "./www/alt/asrNuanceWsCompat.js",
        "type": "asr"
      }
    }
  },
  "devDependencies": {
    "mmir-plugin-exports": "git+https://gitlab-cos.b.dfki.de/mmir/mmir-plugin-exports.git"
  }
}
